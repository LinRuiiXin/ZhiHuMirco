<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.user.dao.InformationDao">

    <select id="getAuthorNewInformation" resultType="information">
        select * from information
        where authorId = #{authorId}
        order by id desc
        limit #{start},#{size}
    </select>
</mapper>
