<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.answer.dao.AnswerSupportDao">
    <insert id="supportAnswer">
        insert into answer_support(answerId,userId,time)
        values(#{answerId},#{userId},now())
    </insert>
    <delete id="unSupportAnswer">
        delete from answer_support
        where answerId = #{answerId}
        and userId = #{userId}
    </delete>
</mapper>