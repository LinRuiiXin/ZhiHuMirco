<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.search.dao.QuestionDao">
    <select id="getAllQuestion" resultType="java.lang.String">
        select name from question
    </select>
    <select id="getQuestionTitle" resultType="java.lang.String">
        select name from question where id = #{id}
    </select>
</mapper>